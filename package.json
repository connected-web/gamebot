{
  "name": "gamebot",
  "version": "1.0.0",
  "description": "Gamebot for slack",
  "main": "index.js",
  "scripts": {
    "start": "npm run gamebot && npm run website",
    "gamebot": "pm2 start index.js --name 'Gamebot' --watch",
    "test": "mocha --recursive",
    "website": "pm2 start website.js --name 'Gamebot Website' --watch",
    "stop": "pm2 delete 'Gamebot' 'Gamebot Website'"
  },
  "keywords": [
    "slack",
    "bot",
    "board",
    "games"
  ],
  "author": "John Beech",
  "license": "ISC",
  "dependencies": {
    "glob": "^7.0.5",
    "slackbots": "^0.5.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "product-monitor": "^2.2.4"
  }
}
